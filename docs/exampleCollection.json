{
    "info": {
      "_postman_id": "9c8f1c1e-9c99-4c88-9e8f-123456789abc",
      "name": "Dummy Social App API",
      "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
      "description": "Complex dummy API collection for testing import functionality"
    },
    "variable": [
      { "key": "baseUrl", "value": "https://api.dummyapp.com" },
      { "key": "authToken", "value": "" },
      { "key": "userId", "value": "" },
      { "key": "postId", "value": "" }
    ],
    "item": [
      {
        "name": "1. Login",
        "request": {
          "method": "POST",
          "header": [{ "key": "Content-Type", "value": "application/json" }],
          "url": {
            "raw": "{{baseUrl}}/auth/login",
            "host": ["{{baseUrl}}"],
            "path": ["auth", "login"]
          },
          "body": {
            "mode": "raw",
            "raw": "{\n  \"email\": \"user@test.com\",\n  \"password\": \"password123\"\n}"
          }
        }
      },
      {
        "name": "2. Get Current User",
        "request": {
          "method": "GET",
          "header": [
            { "key": "Authorization", "value": "Bearer {{authToken}}" }
          ],
          "url": {
            "raw": "{{baseUrl}}/users/me",
            "host": ["{{baseUrl}}"],
            "path": ["users", "me"]
          }
        }
      },
      {
        "name": "3. Update User Profile",
        "request": {
          "method": "PUT",
          "header": [
            { "key": "Authorization", "value": "Bearer {{authToken}}" },
            { "key": "Content-Type", "value": "application/json" }
          ],
          "url": {
            "raw": "{{baseUrl}}/users/{{userId}}",
            "host": ["{{baseUrl}}"],
            "path": ["users", "{{userId}}"]
          },
          "body": {
            "mode": "raw",
            "raw": "{\n  \"name\": \"Updated Name\",\n  \"bio\": \"Just a dummy user\"\n}"
          }
        }
      },
      {
        "name": "4. Create Post",
        "request": {
          "method": "POST",
          "header": [
            { "key": "Authorization", "value": "Bearer {{authToken}}" },
            { "key": "Content-Type", "value": "application/json" }
          ],
          "url": {
            "raw": "{{baseUrl}}/posts",
            "host": ["{{baseUrl}}"],
            "path": ["posts"]
          },
          "body": {
            "mode": "raw",
            "raw": "{\n  \"title\": \"My First Post\",\n  \"content\": \"This is a test post\"\n}"
          }
        }
      },
      {
        "name": "5. Get All Posts (Pagination)",
        "request": {
          "method": "GET",
          "header": [
            { "key": "Authorization", "value": "Bearer {{authToken}}" }
          ],
          "url": {
            "raw": "{{baseUrl}}/posts?page=1&limit=10",
            "host": ["{{baseUrl}}"],
            "path": ["posts"],
            "query": [
              { "key": "page", "value": "1" },
              { "key": "limit", "value": "10" }
            ]
          }
        }
      },
      {
        "name": "6. Get Single Post",
        "request": {
          "method": "GET",
          "header": [
            { "key": "Authorization", "value": "Bearer {{authToken}}" }
          ],
          "url": {
            "raw": "{{baseUrl}}/posts/{{postId}}",
            "host": ["{{baseUrl}}"],
            "path": ["posts", "{{postId}}"]
          }
        }
      },
      {
        "name": "7. Add Comment to Post",
        "request": {
          "method": "POST",
          "header": [
            { "key": "Authorization", "value": "Bearer {{authToken}}" },
            { "key": "Content-Type", "value": "application/json" }
          ],
          "url": {
            "raw": "{{baseUrl}}/posts/{{postId}}/comments",
            "host": ["{{baseUrl}}"],
            "path": ["posts", "{{postId}}", "comments"]
          },
          "body": {
            "mode": "raw",
            "raw": "{\n  \"comment\": \"Nice post!\"\n}"
          }
        }
      },
      {
        "name": "8. Like Post",
        "request": {
          "method": "POST",
          "header": [
            { "key": "Authorization", "value": "Bearer {{authToken}}" }
          ],
          "url": {
            "raw": "{{baseUrl}}/posts/{{postId}}/like",
            "host": ["{{baseUrl}}"],
            "path": ["posts", "{{postId}}", "like"]
          }
        }
      },
      {
        "name": "9. Delete Post",
        "request": {
          "method": "DELETE",
          "header": [
            { "key": "Authorization", "value": "Bearer {{authToken}}" }
          ],
          "url": {
            "raw": "{{baseUrl}}/posts/{{postId}}",
            "host": ["{{baseUrl}}"],
            "path": ["posts", "{{postId}}"]
          }
        }
      },
      {
        "name": "10. Logout",
        "request": {
          "method": "POST",
          "header": [
            { "key": "Authorization", "value": "Bearer {{authToken}}" }
          ],
          "url": {
            "raw": "{{baseUrl}}/auth/logout",
            "host": ["{{baseUrl}}"],
            "path": ["auth", "logout"]
          }
        }
      }
    ]
  }
  